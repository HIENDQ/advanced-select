    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "bootstrap": "^3.4.1",
    "core-js": "^3.6.4",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.0",
